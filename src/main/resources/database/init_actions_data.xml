<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet author="ssz" id="insert_actions">
		<insert tableName="ACTION">
			<column name="id" valueNumeric="1" />
			<column name="name" value="Log in" />
			<column name="label" value="action.label.logIn" />
		</insert>
		<insert tableName="ACTION">
			<column name="id" valueNumeric="2" />
			<column name="name" value="Log out" />
			<column name="label" value="action.label.logOut" />
		</insert>
		<insert tableName="ACTION">
			<column name="id" valueNumeric="3" />
			<column name="name" value="Add user" />
			<column name="label" value="action.label.admin.addUser" />
		</insert>
		<insert tableName="ACTION">
			<column name="id" valueNumeric="4" />
			<column name="name" value="Modify user" />
			<column name="label" value="action.label.admin.modifyUser" />
		</insert>
		<insert tableName="ACTION">
			<column name="id" valueNumeric="5" />
			<column name="name" value="Remove user" />
			<column name="label" value="action.label.admin.removeUser" />
		</insert>
		<insert tableName="ACTION">
			<column name="id" valueNumeric="6" />
			<column name="name" value="Block user" />
			<column name="label" value="action.label.admin.blockUser" />
		</insert>
		<insert tableName="ACTION">
			<column name="id" valueNumeric="7" />
			<column name="name" value="Turn on" />
			<column name="label" value="action.label.dev.turnOn" />
		</insert>
		<insert tableName="ACTION">
			<column name="id" valueNumeric="9" />
			<column name="name" value="Turn off" />
			<column name="label" value="action.label.dev.turnOff" />
		</insert>
	</changeSet>

</databaseChangeLog>